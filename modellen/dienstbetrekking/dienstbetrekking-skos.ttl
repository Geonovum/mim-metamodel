<urn:uuid:1f9180a4-372d-5d57-9c8d-75eb7f9e89d5>
        rdf:type         <http://bp4mc2.org/def/jas#Rechtsbetrekking> , skos:Concept;
        rdfs:label       "Verplichting tot leveren van arbeid"@nl;
        dct:source       <urn:uuid:dd069be6-46e1-5690-9710-3b708effdc86>;
        skos:definition  "Een verplichting tot leveren van arbeid is een verplicht van een [werknemer] om arbeid te leveren ten dienste van een [werkgever]"@nl;
        skos:inScheme    <urn:uuid:a5a10054-2c4e-5435-9ca2-f8bda1f9ed3e>;
        skos:prefLabel   "Verplichting tot leveren van arbeid"@nl;
