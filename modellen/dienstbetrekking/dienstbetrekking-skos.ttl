<urn:uuid:fad11238-dbe3-5aa5-abe4-254893e3dc28>
        rdf:type         <http://bp4mc2.org/def/jas#Rechtssubject> , skos:Concept;
        rdfs:label       "Werkgever"@nl;
        dct:source       <urn:uuid:dd069be6-46e1-5690-9710-3b708effdc86>;
        skos:definition  "Een werkgever is een organisatie die arbeid laat verrichten door een ander in ruil voor loon"@nl;
        skos:inScheme    <urn:uuid:a5a10054-2c4e-5435-9ca2-f8bda1f9ed3e>;
        skos:prefLabel   "Werkgever"@nl .

<urn:uuid:8455df74-ffe1-5178-8042-913bb1d28487>
        rdf:type         <http://bp4mc2.org/def/jas#Rechtssubject> , skos:Concept;
        rdfs:label       "Werknemer"@nl;
        dct:source       <urn:uuid:dd069be6-46e1-5690-9710-3b708effdc86>;
        skos:definition  "Een werknemer is een natuurlijk persoon die arbeid verricht voor een ander in ruil voor loon"@nl;
        skos:inScheme    <urn:uuid:a5a10054-2c4e-5435-9ca2-f8bda1f9ed3e>;
        skos:prefLabel   "Werknemer"@nl .

<urn:uuid:d25a8ec3-ed32-515c-a289-917629d56f2e>
        rdf:type         <http://bp4mc2.org/def/jas#Rechtshandeling> , skos:Concept;
        rdfs:label       "Arbeidsovereenkomst"@nl;
        dct:source       <urn:uuid:dd069be6-46e1-5690-9710-3b708effdc86>;
        skos:definition  "Een arbeidsovereenkomst is een overeenkomst waarbij de ene partij, de [werknemer], zich verbindt in dienst van de andere partij, de [werkgever], tegen loon gedurende zekere tijd arbeid te verrichten."@nl;
        skos:inScheme    <urn:uuid:a5a10054-2c4e-5435-9ca2-f8bda1f9ed3e>;
        skos:prefLabel   "Arbeidsovereenkomst"@nl;
