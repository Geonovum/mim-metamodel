<urn:uuid:b3752389-4a50-5554-9505-55c7b51bd346>
        rdf:type         <http://bp4mc2.org/def/jas#Rechtsbetrekking> , skos:Concept;
        rdfs:label       "Verplichting tot uitbetalen loon"@nl;
        dct:source       <urn:uuid:dd069be6-46e1-5690-9710-3b708effdc86>;
        skos:definition  "Een verplichting tot uitbetalen loon is een verplicht van een [werkgever] om loon uit te betalen aan een [werknemer]"@nl;
        skos:inScheme    <urn:uuid:a5a10054-2c4e-5435-9ca2-f8bda1f9ed3e>;
        skos:prefLabel   "Verplichting tot uitbetalen loon"@nl;
